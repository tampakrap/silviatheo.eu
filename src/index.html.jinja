    <section class="rsvp-section section-padding parallax" data-bg-image="../images/rsvp-bg.jpg" data-speed="7" id="rsvp">
      <div class="container">
        <div class="row">
          <div class="col col-xs-12">
            <div class="section-title-white">
              <div class="vertical-line"><span><div class="fi flaticon-bird"></div></span></div>
              <h2>{% if lang in special_langs %}<span class="{{ lang }}">{{ rsvp_title }}</span>{% else %}{{ rsvp_title }}{% endif %}</h2>
            </div>
          </div>
        </div>
        <div class="row content">
          <div class="col col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
            <p>{{ rsvp_content }}</p>
            <form id="rsvp-form" class="form validate-rsvp-form row" method="post">
              <div class="col col-sm-6">
                <input type="text" name="name" class="form-control" placeholder="{{ rsvp_name }}*" aria-label="{{ rsvp_name }}*">
              </div>
              <div class="col col-sm-6">
                <input type="email" name="email" class="form-control" placeholder="{{ rsvp_email }}*" aria-label="{{ rsvp_email }}*">
              </div>
              <div class="col col-sm-6">
                <input type="text" name="partner-name" class="form-control" placeholder="{{ rsvp_partner }}" aria-label="{{ rsvp_partner }}">
              </div>
              <div class="col col-sm-6">
                <div class="select-wrapper">
                  <select class="form-control" name="kids" aria-label="{{ rsvp_kids }}">
                    <option disabled selected>{{ rsvp_kids }}</option>
                    <option>0</option>
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                  </select>
                </div>
              </div>
              <div class="col col-sm-12">
                <textarea class="form-control" name="message" placeholder="{{ rsvp_message }}" aria-label="{{ rsvp_message }}"></textarea>
              </div>
              <div class="col col-sm-12 submit-btn">
                <button type="submit" class="submit">{{ rsvp_send }}</button>
                <span id="loader">
                  <div class="sk-circle">
                    <div class="sk-circle1 sk-child"></div>
                    <div class="sk-circle2 sk-child"></div>
                    <div class="sk-circle3 sk-child"></div>
                    <div class="sk-circle4 sk-child"></div>
                    <div class="sk-circle5 sk-child"></div>
                    <div class="sk-circle6 sk-child"></div>
                    <div class="sk-circle7 sk-child"></div>
                    <div class="sk-circle8 sk-child"></div>
                    <div class="sk-circle9 sk-child"></div>
                    <div class="sk-circle10 sk-child"></div>
                    <div class="sk-circle11 sk-child"></div>
                    <div class="sk-circle12 sk-child"></div>
                  </div>
                </span>
              </div>
              <div class="col col-md-12 success-error-message">
                <div id="success">{{ rsvp_thankyou }}</div>
                <div id="error"> Error occurred while sending email. Please try again later. </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </section>
