<body id="home">
  <div class="page-wrapper">
    <div class="preloader">
      <div class="inner">
        <span class="icon"><div class="fi flaticon-bird"></div></span>
      </div>
    </div>
    <section class="hero">
      <div class="hero-slider hero-slider-s1">
        <div class="slide-item">
          <img src="../images/slider/slide-1.jpg" alt="Silvia and Theo" class="slider-bg">
        </div>
        <div class="slide-item">
          <img src="../images/slider/slide-2.jpg" alt="Silvia and Theo" class="slider-bg">
        </div>
      </div>
      <div class="wedding-announcement">
        <div class="couple-name-merried-text">
          <!-- htmllint id-class-style="camel" -->
          <h1 class="wow slideInUp" data-wow-duration="1s">Σίλβια &amp; Θοδωρής</h1>
          {%- set time = namespace(value=1) %}
          <!-- htmllint id-class-style=false -->
          <div class="married-text wow fadeIn" data-wow-delay="{{ time.value }}s">
            <h4 class="">
            <!-- htmllint id-class-style="camel" -->
            {%- for letter in weregettingmarried %}
            {%- set time.value = time.value + 0.05 %}
            {%- if letter != ' ' %}
            <span class="wow fadeInUp" data-wow-delay="{{'%0.2f' | format(time.value|float) }}s">{{ letter }}</span>
            {%- else -%}
            <span>&nbsp;</span>
            {%- endif %}
            {%- endfor %}
            <!-- htmllint id-class-style="dash" -->
            </h4>
          </div>
        </div>
        <div class="save-the-date">
          <h4>Save the date</h4>
          <span class="date">{{ date }}</span>
        </div>
      </div>
    </section>
